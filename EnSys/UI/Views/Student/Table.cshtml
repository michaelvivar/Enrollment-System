@model IEnumerable<UI.Models.StudentModel>

<tbody id="data-table">
@foreach (var item in Model)
{
    <tr>
        <td width="20"><a href="@Url.Action("Edit", new { Id = item.Id })" class="glyphicon glyphicon-pencil"></a></td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Level) Year
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SectionCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.Id })
        </td>
    </tr>
}
</tbody>
